{
  "name": "@water-ui/core",
  "version": "1.0.0",
  "description": "Water UI 核心组件库",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "web-components",
    "ui",
    "components",
    "water-ui"
  ],
  "author": "eveningwater(854806732@qq.com)",
  "license": "MIT",
  "dependencies": {
    "@water-ui/icons": "workspace:*",
    "@water-ui/themes": "workspace:*",
    "@water-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "jsdom": "^22.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}