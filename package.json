{
  "name": "water-ui",
  "version": "0.0.1",
  "description": "基于原生Web Components的现代化UI组件库",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "node build.js",
    "build:packages": "pnpm -r build",
    "build:themes": "cd packages/themes && pnpm build:themes",
    "lint": "pnpm -r lint",
    "example": "cd examples && python3 -m http.server 8080"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.0.0"
} 